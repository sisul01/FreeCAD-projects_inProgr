<script type="text/javascript">





$(function() {



// Thickbox


$('.thickbox').click(function(e){e.preventDefault();var imgPath = $(this).attr("href");$('#modal2 #myModalLabel').html(this.title);$('#modal2 .modal-body').html('<img src="' +imgPath+ '" alt="" class="center-block img-responsive" />');$("#modal2").modal('show');
$('#modal2').on('hidden', function() {$(this).removeData('modal');});});



$('.contentbox').click(function(ev){ev.preventDefault();var target = $(this).attr("href");$('#modal3 #myModalLabel').html(this.title);$('#modal3').modal({remote: target});$("#modal3").modal('show');$('#modal3').on('hidden', function() {$(this).removeData('modal');});});

var c = document.cookie;

$('.collapsebox').each(function () {if (this.id) { var pos = c.indexOf(this.id + "_collapse_in="); if (pos > -1) { c.substr(pos).split('=')[1].indexOf('false') ? $(this).addClass('in') : $(this).removeClass('in');}}}).on('hidden shown', function () {if (this.id) { document.cookie = this.id + "_collapse_in=" + $(this).hasClass('in');}});

$('#myCarousel').carousel({interval: 5000})
$('.dropdown.men').hover(function() { $(this).addClass('open');}, function() {$(this).removeClass('open');});

$('div.accordion-body').on('shown', function () { $(this).parent("div").find(".icon-chevron-down").removeClass("icon-chevron-down").addClass("icon-chevron-up");});

$('div.accordion-body').on('hidden', function () { $(this).parent("div").find(".icon-chevron-up").removeClass("icon-chevron-up").addClass("icon-chevron-down");});

$('.payment_block .radio').on('click', function() {
    $('.payment_block .radio').removeClass('active');
    $(this).addClass('active');
    $(this).prev('.radio input').prop('checked', true);
});

$(".same").height( $(".same_height").height() );

 //-- Click on QUANTITY
            $(".btn-minus").on("click",function(){
                var now = $(".section > .form-control:text").val();
                if ($.isNumeric(now)){
                    if (parseInt(now) -1 > 0){ now--;}
                    $(".section > .form-control:text").val(now);
                }else{
                    $(".section > .form-control:text").val("1");
                }
            })
            $(".btn-plus").on("click",function(){
                var now = $(".section > .form-control:text").val();
                if ($.isNumeric(now)){
                    $(".section > .form-control:text").val(parseInt(now)+1);
                }else{
                    $(".section > .form-control:text").val("1");
                }
            })
          });
</script>
